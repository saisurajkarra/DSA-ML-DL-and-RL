        "def majority(arr,n):\n",
        "  count = 1\n",
        "  maj = arr[0]\n",
        "  for i in range(1,n):\n",
        "    if(maj == arr[i]):\n",
        "      count+=1\n",
        "    else:\n",
        "      count-=1\n",
        "    \n",
        "    if(count==0):\n",
        "      maj = arr[i]\n",
        "      count = 1\n",
        "  return maj\n",
        "    \n",
        "    \n",
        "\n",
        "  \n",
        "   \n",
        "arr = [2, 3, 3, 2, 3, 5]\n",
        "majority(arr, len(arr))"
